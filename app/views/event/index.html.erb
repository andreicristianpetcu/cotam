<h1>Events</h1>

 <div id="map"></div>

<ul>
  <% for @event in @events %>
    <%= link_to @event.name, @event.url %>
    </br>
    </br>
  <% end %>
</ul>

<script>
  var map = L.map('map').setView([51.505, -0.09], 13);
  L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png').addTo(map);
</script>
