<div id="map">
  <div id="visible" class="large-3 small-4 columns">
    <div class="panel radius">
      <ul>
        <% for @event in @events %>
          <%= link_to @event.short_name.html_safe, @event.url %>
          <%= link_to "->", @event, :class=>"admin_event"%>
          </br>
          </br>
        <% end %>
      </ul>
    </div>
    <a class="button expand" href="/admin/event/new">Add your event</a>
  </div>
  <div class="large-9 small-8 columns"></div>
</div>
